{
  "name": "angular-ngx-treeview",
  "version": "0.0.0",
  "license": "MIT",
  "description": "An Angular treeview component with checkbox",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:lib": "ng build --configuration production ngx-treeview",
    "build:demo": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --configuration production --base-href /ngx-treeview/ && node gh-pages.js",
    "build_lib": "ng build --configuration production ngx-treeview",
    "npm_pack": "cd ./dist/ngx-treeview && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm run package && cd ./dist/ngx-treeview && npm publish --access public"
  },
  "private": true,
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "ngx-treeview",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "author": "Leo Vo <leovo2708@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yberion/ngx-treeview.git"
  },
  "bugs": {
    "url": "https://github.com/yberion/ngx-treeview/issues"
  },
  "dependencies": {
    "@angular/animations": "~13.0.3",
    "@angular/common": "~13.0.3",
    "@angular/compiler": "~13.0.3",
    "@angular/core": "~13.0.3",
    "@angular/forms": "~13.0.3",
    "@angular/platform-browser": "~13.0.3",
    "@angular/platform-browser-dynamic": "~13.0.3",
    "@angular/platform-server": "~13.0.3",
    "@angular/router": "~13.0.3",
    "bootstrap": "^4.5.3",
    "lodash": "^4.17.15",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular-devkit/core": "~13.0.4",
    "@angular-devkit/schematics": "~13.0.4",
    "@angular/cli": "^13.0.4",
    "@angular/compiler-cli": "~13.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.47",
    "codelyzer": "^6.0.0-next.1",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  }
}
